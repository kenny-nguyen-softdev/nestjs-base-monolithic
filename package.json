{
  "name": "liem-lan-cms-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "echo '\n Starting build...' && nest build && echo '\n Build completed successfully!'",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "g:module": "nest g module",
    "g:controller": "nest g controller",
    "g:service": "nest g service",
    "g:resource": "nest g resource",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "npx typeorm-ts-node-commonjs schema:sync",
    "typeorm:cache": "npx typeorm-ts-node-commonjs cache:clear",
    "schema:drop": "npx typeorm-ts-node-commonjs -d ./src/core/database/data-source.ts",
    "migration:generate:name": "ts-node scripts/migration-gen.ts",
    "migration:create:name": "ts-node scripts/migration-gen.ts",
    "migration:show": "npx typeorm-ts-node-commonjs migration:show -d src/core/database/data-source.ts",
    "migration:migrate": "npx typeorm-ts-node-commonjs migration:run -d src/core/database/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/core/database/data-source.ts",
    "seed:generate:name": "ts-node scripts/migration-gen.ts",
    "seed:create:name": "ts-node scripts/migration-gen.ts",
    "seed:show": "npx typeorm-ts-node-commonjs migration:show -d src/core/database/seed-data-source.ts",
    "seed:migrate": "npx typeorm-ts-node-commonjs migration:run -d src/core/database/seed-data-source.ts",
    "seed:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/core/database/seed-data-source.ts",
    "prepare": "husky",
    "eslint": "npx eslint src/**/*.ts"
  },
  "dependencies": {
    "@aws-sdk/client-route-53": "^3.777.0",
    "@aws-sdk/client-s3": "^3.777.0",
    "@aws-sdk/s3-request-presigner": "^3.777.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.12",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/typeorm": "^11.0.0",
    "@sparticuz/chromium": "^138.0.2",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parser": "^3.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "nest-winston": "^1.10.2",
    "nodemailer": "^6.10.1",
    "numeral": "^2.0.6",
    "pg": "^8.14.1",
    "probe-image-size": "^7.2.3",
    "puppeteer-core": "^24.16.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.33.5",
    "typeorm": "^0.3.21",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.12",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/handlebars": "^4.0.40",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.14",
    "@types/nodemailer": "^6.4.17",
    "@types/numeral": "^2.0.5",
    "@types/probe-image-size": "^7.2.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.20.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,md}": "prettier --write --ignore-unknown"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
